import flate2

# Compress data with zlib
fn zlib_compress(data: string) {
    return flate2.zlib_compress(data)
}

# Decompress data with zlib
fn zlib_decompress(data: string) {
    return flate2.zlib_decompress(data)
}

# Compress with gzip
fn gzip_compress(data: string) {
    return flate2.gzip_compress(data)
}
