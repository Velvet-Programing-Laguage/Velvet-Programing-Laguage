import pytorch

# Load a PyTorch model
fn pt_load_model(path: string) {
    return pytorch.load_model(path)
}

# Run inference
fn pt_predict(model: string, data: string) {
    return pytorch.predict(model, data)
}

# Fine-tune a model
fn pt_finetune(model: string, dataset: string, epochs: int) {
    return pytorch.finetune(model, dataset, epochs)
}
