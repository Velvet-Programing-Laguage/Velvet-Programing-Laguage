# Wrapper for Python's requests library
fn requests_get(url) {
    return python_requests(url, "get")
}

fn requests_post(url, data) {
    return python_requests(url, "post", data)
}

fn requests_put(url, data) {
    return python_requests(url, "put", data)
}

fn requests_delete(url) {
    return python_requests(url, "delete")
}
