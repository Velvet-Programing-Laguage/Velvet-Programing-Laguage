import axios

# HTTP GET request
fn axios_get(url: string, headers: map) {
    return axios.get(url, headers)
}

# HTTP POST request
fn axios_post(url: string, data: map, headers: map) {
    return axios.post(url, data, headers)
}

# HTTP retry with timeout
fn axios_retry(url: string, retries: int, timeout: int) {
    return axios.retry(url, retries, timeout)
}
