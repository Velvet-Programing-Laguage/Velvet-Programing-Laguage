# WebSocket module for real-time communication
import websocket

# Start WebSocket server
fn ws_server(port: int) {
    return websocket.server(port)
}

# Connect to WebSocket server
fn ws_connect(url: string) {
    return websocket.connect(url)
}

# Send message
fn ws_send(client: string, message: string) {
    return websocket.send(client, message)
}
