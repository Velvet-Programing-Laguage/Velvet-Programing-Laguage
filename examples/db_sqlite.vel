# SQLite database module
import sqlite

# Connect to a database
fn sqlite_connect(db_path: string) {
    return sqlite.connect(db_path)
}

# Execute a query
fn sqlite_query(db: string, query: string) {
    return sqlite.query(db, query)
}

# Insert data
fn sqlite_insert(db: string, table: string, data: map) {
    return sqlite.insert(db, table, data)
      }
